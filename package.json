    "@stripe/stripe-js": "^4.6.0",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/ssr": "^0.5.1",
    "@supabase/supabase-js": "^2.45.4",
    "@tanstack/react-query": "^5.59.0",

    "@vercel/edge": "^1.1.2",
    "@vercel/edge-config": "^1.3.0",
    "classnames": "^2.5.1",
    "clipboard-copy": "^4.0.1",
    "dotenv": "^16.4.5",
    "eslint-config-next": "^14.2.14",
    "googleapis": "^144.0.0",
    "ioredis": "^5.4.1",
    "next": "^14.2.14",
    "next-themes": "^0.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-geocode": "^0.2.3",
    "react-hook-form": "^7.53.0",
    "react-icons": "^5.3.0",
    "react-mentions": "^4.4.10",
    "react-responsive": "^10.0.0",
    "react-toastify": "^10.0.5",
    "stripe": "^17.0.0",
    "typescript": "^5.6.2",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.3.1",
    "@tanstack/eslint-plugin-query": "^5.58.1",
    "@types/node": "^22.7.4",
    "@types/node-fetch": "^2.6.11",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "@types/react-geocode": "^0.2.4",
    "@types/react-mentions": "^4.4.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "postcss": "^8.4.47",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "supabase": "^1.200.3",
    "tailwindcss": "^3.4.13"
  },
  "prettier": {
    "arrowParens": "always",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "none"
  }
